{
  "name": "DOODLE-DOCZ",
  "productName": "DOODLE-DOCZ",
  "version": "1.0.0",
  "description": "Reactive Docs",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "backend-dev": "nodemon server/index.js --exec babel-node --presets es2015,stage-2",
    "backend-build": "babel server -d server-dist --presets es2015,stage-2",
    "backend-serve": "node server-dist/index.js"
  },
  "keywords": [],
  "author": "franky",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": [
          "server",
          "server-dist"
        ]
      },
      "electronWinstallerConfig": {
        "name": "DOODLE DOCS"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "week09reactivedocs"
      }
    }
  },
  "dependencies": {
    "draft-js": "^0.10.5",
    "draft-js-buttons": "^2.0.1",
    "draft-js-color-picker": "^1.0.2",
    "draft-js-counter-plugin": "^2.0.1",
    "draft-js-emoji-plugin": "^2.0.1",
    "draft-js-export-html": "^1.2.0",
    "draft-js-hashtag-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.4",
    "draft-js-static-toolbar-plugin": "^2.0.1",
    "draft-js-undo-plugin": "^2.0.1",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "material-ui": "^0.20.0",
    "moment": "^2.21.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-modal": "^3.3.2",
    "react-router-dom": "^4.2.2",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electron": "^1.8.4",
    "electron-forge": "^5.1.1",
    "electron-prebuilt-compile": "1.8.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.2",
    "socket.io": "^2.0.4"
  }
}
